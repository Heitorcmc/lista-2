#include <stdio.h>

int main(void) {
  printf("Digite um n√∫mero inteiro e positivo: \n");
  int n = 0;
  float s = 0;

  scanf("%d", &n);
//o for vai ser usado para realizar a operacao o numero de vezes que o usuario quiser
  for (float x = 1; x <= n; x++) {
    s += 1 / x;
    printf("%f \n", s);
  }
  printf("Resultado: %f", s);
  return 0;
}
